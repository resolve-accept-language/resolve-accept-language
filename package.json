{
  "name": "resolve-accept-language",
  "version": "3.1.5",
  "description": "Resolve the preferred locale based on the value of an `Accept-Language` HTTP header.",
  "keywords": [
    "accept-language",
    "RFC 4647",
    "locale",
    "language tags",
    "RFC 4646",
    "BCP 47",
    "RFC 2616",
    "resolve",
    "detect",
    "intl",
    "i18n",
    "internationalization"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resolve-accept-language/resolve-accept-language.git"
  },
  "license": "MIT",
  "author": "resolve-accept-language",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "types": "./lib/cjs/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run prettier && npm run lint-fix && rm -Rf ./lib && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && echo '{ \"type\": \"commonjs\" }' > lib/cjs/package.json && tsc -p tsconfig.build-scripts.json && node lib/build-scripts/build.js && npm run test",
    "depcheck": "depcheck",
    "lint-fix": "eslint --fix .",
    "prettier": "prettier --write .",
    "release": "dotenv -- release-it --only-version",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@release-it/conventional-changelog": "8.0.1",
    "@types/jest": "29.5.12",
    "@types/node": "^22.5.2",
    "@typescript-eslint/eslint-plugin": "8.3.0",
    "@typescript-eslint/parser": "8.3.0",
    "depcheck": "^1.4.7",
    "dotenv-cli": "7.4.2",
    "eslint": "9.9.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-node": "0.3.9",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import-x": "^4.1.1",
    "eslint-plugin-jest": "28.8.1",
    "eslint-plugin-jsdoc": "50.2.2",
    "eslint-plugin-json-files": "4.4.1",
    "eslint-plugin-prefer-arrow-functions": "3.4.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-tsdoc": "0.3.0",
    "eslint-plugin-unicorn": "55.0.0",
    "globals": "15.9.0",
    "jest": "29.7.0",
    "jsonc-eslint-parser": "2.4.0",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "4.0.0",
    "prettier-plugin-sh": "0.14.0",
    "release-it": "17.6.0",
    "terser": "^5.31.6",
    "ts-jest": "29.2.5",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": "^14.18.1 || >=16.0.0"
  }
}
