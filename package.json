{
  "name": "resolve-accept-language",
  "version": "1.0.50",
  "description": "Resolve the preferred locale based on the value of an `Accept-Language` HTTP header.",
  "author": "Avansai (https://avansai.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Avansai/resolve-accept-language.git"
  },
  "main": "lib/resolve-accept-language.js",
  "types": "lib/resolve-accept-language.d.ts",
  "scripts": {
    "build": "npm run lint && npm test && rm -Rf ./lib && tsc",
    "test": "jest --coverage",
    "lint": "eslint . --fix",
    "prettier-check": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "release": "dotenv -- release-it --only-version"
  },
  "license": "MIT",
  "files": [
    "lib"
  ],
  "keywords": [
    "accept-language",
    "RFC 4647",
    "locale",
    "language tags",
    "RFC 4646",
    "BCP 47",
    "RFC 2616",
    "resolve",
    "detect",
    "intl",
    "i18n",
    "internationalization"
  ],
  "devDependencies": {
    "@release-it/conventional-changelog": "^4.1.0",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "dotenv-cli": "^5.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^27.5.1",
    "prettier": "2.5.1",
    "release-it": "^14.12.4",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
